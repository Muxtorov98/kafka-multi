{
  "name": "muxtorov98/kafka-multi",
  "description": "Universal Kafka for PHP (Yii2, Laravel, Symfony) â€” auto-discovery, retry, multi-group",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-rdkafka": "*",
    "psr/log": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Muxtorov98\\Kafka\\": "src/Core/",
      "Muxtorov98\\Kafka\\Support\\": "src/Support/",
      "Muxtorov98\\Kafka\\Bridge\\Yii2\\": "src/Bridge/Yii2/",
      "Muxtorov98\\Kafka\\Bridge\\Laravel\\": "src/Bridge/Laravel/",
      "Muxtorov98\\Kafka\\Bridge\\Symfony\\": "src/Bridge/Symfony/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Muxtorov98\\Kafka\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Muxtorov98\\Kafka\\Bridge\\Laravel\\KafkaServiceProvider"
      ]
    }
  },
  "exclude-from-classmap": [
    "src/Bridge/Laravel/Commands/KafkaWorkCommand.php"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}