{
  "name": "muxtorov98/kafka-multi",
  "description": "Universal Kafka for PHP (Yii2, Laravel, Symfony) â€” auto-discovery, retry, DLQ, multi-group consumer",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-rdkafka": "*",
    "psr/log": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Muxtorov98\\Kafka\\": "src/Core/",
      "Muxtorov98\\Kafka\\Support\\": "src/Support/",
      "Muxtorov98\\Kafka\\Bridge\\Yii2\\": "src/Bridge/Yii2/",
      "Muxtorov98\\Kafka\\Bridge\\Laravel\\": "src/Bridge/Laravel/",
      "Muxtorov98\\Kafka\\Bridge\\Symfony\\": "src/Bridge/Symfony/",
      "Muxtorov98\\Kafka\\Installer\\": "src/Installer/"
    },
    "exclude-from-classmap": [
      "src/Bridge/Laravel/Commands/"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Muxtorov98\\Kafka\\Bridge\\Laravel\\KafkaServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Muxtorov98\\Kafka\\Installer\\ComposerInstaller::postInstall"
    ],
    "post-update-cmd": [
      "Muxtorov98\\Kafka\\Installer\\ComposerInstaller::postUpdate"
    ],
    "post-autoload-dump": [
      "Muxtorov98\\Kafka\\Installer\\ComposerInstaller::postAutoload"
    ],
    "pre-remove-cmd": [
      "Muxtorov98\\Kafka\\Installer\\ComposerInstaller::preRemove"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}